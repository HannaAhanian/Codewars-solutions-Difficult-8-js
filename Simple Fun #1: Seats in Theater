function seatsInTheater(nCols, nRows, col, row) {
  let totalSeats = nCols * nRows;
  let newColNumber = nCols - col + 1;
  let newRowNumber = nRows - row;
  let peopleBehindMe = newColNumber * newRowNumber;
  
  return peopleBehindMe;
}
